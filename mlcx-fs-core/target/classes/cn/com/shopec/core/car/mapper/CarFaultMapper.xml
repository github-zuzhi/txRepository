		</if>
		<if test="driverId!=null and driverId!=''">
			DRIVER_ID=#{driverId,jdbcType = VARCHAR},
		</if>
		<if test="driverName!=null and driverName!=''">
			DRIVER_NAME=#{driverName,jdbcType = VARCHAR},
		</if>
		<if test="memo!=null and memo!=''">
