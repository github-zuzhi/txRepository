		CAR_MODEL_ID,
		CAR_MODEL_NAME,
		CITY_ID,
		CITY_NAME,
		RECORD_FAULT_TIME,
		FAULT_LOCATION,
		FAULT_LEVEL,
		PROCESSING_STATUS,
		USE_CAR_TYPE,
		DOCUMENT_NO,
		DRIVER_ID,
		DRIVER_NAME,
		MEMO,
		CREATE_TIME,
		UPDATE_TIME,
		OPERATOR_TYPE,
		OPERATOR_ID
	) values (
		<if test="faultId!=null">
		#{faultId,jdbcType = VARCHAR},
		</if>
		#{carNo,jdbcType = VARCHAR},
		#{carPlateNo,jdbcType = VARCHAR},
		#{carModelId,jdbcType = VARCHAR},
