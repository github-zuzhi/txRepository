<sql id="listCommonWhere">
	<if test="q!=null">
		<if test="q.terminalDeviceNo!=null and q.terminalDeviceNo!=''">
			AND TERMINAL_DEVICE_NO=#{q.terminalDeviceNo}
		</if>
		<if test="q.deviceSn!=null and q.deviceSn!=''">
			AND DEVICE_SN like '%${q.deviceSn}%'
