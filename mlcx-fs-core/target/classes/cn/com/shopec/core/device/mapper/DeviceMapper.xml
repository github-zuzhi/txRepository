		</if>
		<if test="q.signalStrength!=null ">
			AND SIGNAL_STRENGTH=#{q.signalStrength}
		</if>
		<if test="q.signalStrengthLevel!=null ">
			AND SIGNAL_STRENGTH_LEVEL=#{q.signalStrengthLevel}
		</if>
		<if test="q.lastReportingTime!=null ">
