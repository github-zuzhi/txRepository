	<select id="getWorkerNs" parameterType="Query" resultMap="Worker">
		select
		<include refid="AllColumnlist" />
		from t_worker
		<where>
			<if test="q!=null">
				<if test="q.nameOrempNo!=null and q.nameOrempNo!=''">
					AND WORKER_NAME like '%${q.nameOrempNo}%' OR EMP_NO like
